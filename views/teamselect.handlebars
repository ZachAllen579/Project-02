
    <header>
      <h1> <display:block><strong>Teams you've used</strong> </display:block> </h1>
      <div class="card">
        <ul>
            {{#each teamsUsed as |used|}}
             {{!-- changed from teamsUsed --}}
            {{!-- test --}}
            <li> {{ used.name }} </li>
            {{/each}}
          </ul>
      </div>
      <div class="card">
          <form>
            <fieldset>
              <legend><h1><strong>Teams available to you</strong></h1>
              </legend>
              {{#each allTeams}}
              <input type="radio" id={{this.id}} name="availableTeams" value={{this.name}}>
              <label for={{this.name}}>{{this.name}}</label>
              </br>
              {{/each}}
           </fieldset>
            <div class="form-group">
              <button class="border-solid border-2 border-gray-800 rounded-md text-white bg-gray-800 hover:bg-lightblue" type="submit" id="teamSelectBtn">Select Team</button>
            </div>
          </form>
      </div>
     </header>

 <script src="./js/teamSelect.js"></script>